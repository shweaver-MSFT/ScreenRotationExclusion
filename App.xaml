<Application
    x:Class="ScreenRotationExclusion.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:ScreenRotationExclusion"
    xmlns:converters="using:ScreenRotationExclusion.Converters">

    <Application.Resources>
        <!-- Converters -->
        <converters:EnumToStringConverter x:Key="EnumToStringConverter" />
        <converters:RadioConverter x:Key="RadioConverter" />

        <!-- Enums -->
        <local:OrientationOrigins x:Key="OrientationOrigins_Auto">Auto</local:OrientationOrigins>
        <local:OrientationOrigins x:Key="OrientationOrigins_Native">Native</local:OrientationOrigins>
        <local:OrientationOrigins x:Key="OrientationOrigins_AutoRotationPreference">AutoRotationPreference</local:OrientationOrigins>
    </Application.Resources>

</Application>
